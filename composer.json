{
    "require": {
        "php": "^7.2",
        "ext-json": "*",
        "symfony/console": "^4.4",
        "doctrine/orm": "^2.7",
        "doctrine/annotations": "^1.8",
        "symfony/dotenv": "^4.4",
        "ext-ctype": "*"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        },
        "classmap": ["src"]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        },
        "classmap": ["tests"]
    },
    "require-dev": {
        "phpunit/phpunit": "^8"
    }
}
